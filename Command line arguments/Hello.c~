 // Addition of 2 numbers by command line arguments.
 
 #include<stdio.h>
 #include<stdlib.h>

 int main(int argc,char *argv[])
  { 
    int i = 0 ;
    printf("Total Number of command line Arguments are : %d \n",argc);
    
    for(i=0 ; i<argc ; i++)
     {
       printf("%s \n",argv[i]);
     }
     
    return 0;
  }
